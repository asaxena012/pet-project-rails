<h1>Pets#edit</h1>
<p>Find me in app/views/pets/edit.html.erb</p>

<h1>Edit pet</h1>

<%= form_with model: @pet, url: pet_path, local: true do |form| %>
  <p>
    <%= form.label :age %><br>
    <%= form.text_field :age %><br>
  </p>
  <p>
    <%= form.label :name %><br>
    <%= form.text_area :name %><br>
  </p>
  <p>
    <%= form.label :description %><br>
    <%= form.text_area :description %><br>
  </p>
  <p>    
    <%= form.submit %>
  </p>
<% end %>